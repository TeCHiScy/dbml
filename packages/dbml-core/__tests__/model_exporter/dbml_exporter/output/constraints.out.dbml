Table "User" {
  "balance" int [constraint: `balance > 0`]
  "name" TEXT [constraint: `LEN(name) > 0`]
  "email" TEXT

  Constraints {
    `balance < 10000000` [name: 'not_too_much_money']
    `LEN(name) < 256` [name: 'name_not_too_long']
    `REGEXP_LIKE(email, '^[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,}$')`
  }
}

Table "User2" {
  "balance" int

  Constraints {
    `balance < 10000000` [name: 'not_too_much_money']
  }
}

Table "User3" {
  "balance" int [constraint: `balance > 0`, constraint: `balance < 10000000`]
}
